{
  "$schema": "https://raw.githubusercontent.com/microsoft/Microsoft-Fabric-tools-workload/main/Workload/app/items/PackageInstallerItem/package/package.schema.json",
  "id": "DAXPerformanceTesting",
  "displayName": "DAX Performance Testing Admin Monitoring",
  "icon": "/assets/items/PackageInstallerItem/DAXPerformanceTesting/icon.png",
  "description": "Adds a notebook and data that is designed to help you with DAX Performance Testing.",
  "items": [
    {
      "displayName": "DAX_Performance_Testing_LH",
      "description": "Stores the performance data and the queries that should be evaluated.",
      "type": "Lakehouse",
      "data": {
        "files":[
          {
            "path": "/Files/DAXQueries.xlsx",
            "payloadType": "AssetLink",
            "payload": "/assets/items/PackageInstallerItem/DAXPerformanceTesting/DAX_Performance_Testing_LH/data/DAXQueries.xlsx"
          }
        ]
      }
    },
    {
      "displayName": "DAX_Performance_Testing_NB",
      "description": "Notebook that runs the DAX Performance Testing.",
      "type": "Notebook",
      "definition": {
        "format": "ipynb",
        "parts": [
          {
            "path": "notebook-content.ipynb",
            "payloadType": "Link",
            "payload": "https://raw.githubusercontent.com/microsoft/fabric-toolbox/refs/heads/main/tools/DAXPerformanceTesting/DAXPerformanceTesting.ipynb"
          }
        ]
      }
    }
  ]
}