{
  "id": "SemanticModelAudit",
  "displayName": "Semantic Model Audit",
  "icon": "/assets/items/PackageInstallerItem/SemanticModelAudit/icon.png",
  "description": "Adds a notebook and data and report for Semantic Model Audit. Schedules the notebook to run daily.",
  "items": [
    {
      "displayName": "SemanticModelAudit_NB",
      "description": "Notebook that runs the Semantic Model Audit.",
      "type": "Notebook",
      "definition": {
        "format": "ipynb",
        "parts": [
          {
            "path": "notebook-content.ipynb",
            "payloadType": "Link",
            "payload": "https://raw.githubusercontent.com/microsoft/fabric-toolbox/refs/heads/main/tools/SemanticModelAudit/notebook/SemanticModelAudit.ipynb"
          }
        ]
      },
      "schedules": [
        {
          "enabled": true,
          "jobType": "RunNotebook",
          "configuration": {
            "type": "Cron",
            "startDateTime": "2024-01-01T00:00:00",
            "endDateTime": "2100-12-31T23:59:00",
            "localTimeZoneId": "Central Standard Time",
            "interval": 240
          }
        }
      ]
    },
    {
      "displayName": "SemanticModelAudit_Report",
      "description": "Report that summarizes the results of the Semantic Model Audit.",
      "type": "Report"
    }
  ]
}