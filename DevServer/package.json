{
  "name": "fabric-devserver",
  "version": "1.0.0",
  "description": "Development server for Microsoft Fabric Frontend Developer Sample App",
  "scripts": {
    "start": "env-cmd -f .env.dev webpack serve --open --config webpack.config.js",
    "start:codespace": "node --max-old-space-size=4096 ./node_modules/.bin/env-cmd -f .env.dev webpack serve --open --config webpack.config.js --no-hot",
    "build:test": "env-cmd -f .env.test webpack --config webpack.config.js",
    "build:prod": "env-cmd -f .env.prod webpack --config webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^8.12.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "nuget-bin": "^4.0.0",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^5.0.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.0"
  }
}